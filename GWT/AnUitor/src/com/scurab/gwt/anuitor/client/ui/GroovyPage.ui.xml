<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	.result {
		font-family: monospace;
	}
	</ui:style>	
		<g:VerticalPanel width="100%" ui:field="root">			
			<g:HTMLPanel ui:field="code" />
			<g:HorizontalPanel>
				<g:Button text="Execute" ui:field="executeCode" />
				<g:Button text="Clear Result" ui:field="clearResult" />
			</g:HorizontalPanel>
			<g:TextArea ui:field="result" styleName="{style.result}" width="99.7%"/>			
		</g:VerticalPanel>	
</ui:UiBinder> 